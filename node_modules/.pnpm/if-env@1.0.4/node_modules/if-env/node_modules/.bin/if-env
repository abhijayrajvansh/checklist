#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/abhijayrajvansh/Desktop/checklist/node_modules/.pnpm/if-env@1.0.4/node_modules/if-env/bin/node_modules:/Users/abhijayrajvansh/Desktop/checklist/node_modules/.pnpm/if-env@1.0.4/node_modules/if-env/node_modules:/Users/abhijayrajvansh/Desktop/checklist/node_modules/.pnpm/if-env@1.0.4/node_modules:/Users/abhijayrajvansh/Desktop/checklist/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/abhijayrajvansh/Desktop/checklist/node_modules/.pnpm/if-env@1.0.4/node_modules/if-env/bin/node_modules:/Users/abhijayrajvansh/Desktop/checklist/node_modules/.pnpm/if-env@1.0.4/node_modules/if-env/node_modules:/Users/abhijayrajvansh/Desktop/checklist/node_modules/.pnpm/if-env@1.0.4/node_modules:/Users/abhijayrajvansh/Desktop/checklist/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/if-env.js" "$@"
else
  exec node  "$basedir/../../bin/if-env.js" "$@"
fi
